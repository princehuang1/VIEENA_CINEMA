sql><sql name="SQL 1*">-- 3. FF7 Rebirth (更新為 3 筆)
UPDATE Games SET features = '[
    {
        &quot;title&quot;: &quot;廣闊的世界等待探索&quot;,
        &quot;desc&quot;: &quot;走出米德加，踏入綠意盎然的廣大世界。騎乘陸行鳥穿越草原、攀越山脈，探索這個星球上未知的角落與隱藏的寶藏。&quot;,
        &quot;image&quot;: &quot;/posters/3-A.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;進化的戰鬥系統&quot;,
        &quot;desc&quot;: &quot;結合動作與策略的戰鬥系統更加進化。與夥伴發動強力的聯手攻擊，利用地形與弱點屬性，在激烈的戰鬥中取得優勢。&quot;,
        &quot;image&quot;: &quot;/posters/3-B.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;命運的糾葛&quot;,
        &quot;desc&quot;: &quot;克勞德與夥伴們追逐賽菲罗斯的旅程進入高潮。未知的命運正在前方等待，他們的羈絆將成為對抗毀滅命運的唯一希望。&quot;,
        &quot;image&quot;: &quot;/posters/3-C.jpg&quot;
    }
]' WHERE gameId = 3;

-- 5. 劍星 (更新為 3 筆)
UPDATE Games SET features = '[
    {
        &quot;title&quot;: &quot;奪回地球的戰爭&quot;,
        &quot;desc&quot;: &quot;扮演第7空降小隊成員伊芙，從神祕的孽奇拔手中奪回已成廢墟的地球。在這場攸關人類存亡的戰爭中，揭開隱藏的真相。&quot;,
        &quot;image&quot;: &quot;/posters/5-A.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;華麗流暢的戰鬥&quot;,
        &quot;desc&quot;: &quot;體驗行雲流水般的戰鬥，利用精準的格擋與閃避，施展毀滅性的連擊與強大的貝塔技能，將敵人華麗地斬殺。&quot;,
        &quot;image&quot;: &quot;/posters/5-B.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;迎戰強大敵人&quot;,
        &quot;desc&quot;: &quot;挑戰強勢逼人的頭目戰，感受壓迫感與成就感交織的極致對決。&quot;,
        &quot;image&quot;: &quot;/posters/5-C.jpg&quot;
    }
]' WHERE gameId = 5;

-- 7. FF16 (更新為 3 筆)
UPDATE Games SET features = '[
    {
        &quot;title&quot;: &quot;召喚獸大戰&quot;,
        &quot;desc&quot;: &quot;親身體驗召喚獸之間的史詩級對決。操控巨大的召喚獸進行震撼天地的戰鬥，感受PS5帶來的極致視覺衝擊。&quot;,
        &quot;image&quot;: &quot;/posters/7-A.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;克萊夫的復仇之路&quot;,
        &quot;desc&quot;: &quot;身為羅札利亚公國的第一王子，克萊夫誓言向毀滅國家的黑暗伊弗利特復仇。這是一段跨越數十年的壯闊史詩。&quot;,
        &quot;image&quot;: &quot;/posters/7-B.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;聖光之都的陰影&quot;,
        &quot;desc&quot;: &quot;踏入聖布雷克神聖帝國華麗壯闊的城堡，在金碧輝煌的外表下，潛藏著足以撼動世界的陰謀與權力角逐。探索神聖與黑暗並存的國度，揭開帝國真正的面貌。&quot;,
        &quot;image&quot;: &quot;/posters/7-C.jpg&quot;
    }
]' WHERE gameId = 7;

-- 10. 艾爾登法環 (更新為 3 筆)
UPDATE Games SET features = '[
    {
        &quot;title&quot;: &quot;交界地的史詩&quot;,
        &quot;desc&quot;: &quot;由宮崎英高與喬治·R·R·馬丁聯手打造的奇幻世界。探索廣闊且充滿危險的交界地，揭開艾爾登法環破碎背後的真相。&quot;,
        &quot;image&quot;: &quot;/posters/10-A.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;自由探索與發現&quot;,
        &quot;desc&quot;: &quot;騎乘靈馬托雷特穿越平原、沼澤與城堡，依照自己的步調探索這個世界。每一個角落都隱藏著致命的敵人或強大的寶藏。&quot;,
        &quot;image&quot;: &quot;/posters/10-B.jpg&quot;
    },
    {
        &quot;title&quot;: &quot;法環的指引&quot;,
        &quot;desc&quot;: &quot;跟隨艾爾登法環的指引，尋找散落在交界地的大盧恩。&quot;,
        &quot;image&quot;: &quot;/posters/10-C.jpg&quot;
    }
]' WHERE gameId = 10;</sql><current_tab id="0"/></tab_sql></sqlb_project>
